        ]
      },
      "bySummonerId": {
        "path": "/lol/summoner/v4/summoners/{encryptedSummonerId}",
        "method": "GET",
        "param": [
          "{encryptedSummonerId}"
        ]
      },
      "byPuuid": {
        "path": "/lol/summoner/v4/summoners/by-puuid/{encryptedPUUID}",
        "method": "GET",
        "param": [
          "{encryptedPUUID}"
        ]
      },
      "byAccount": {
        "path": "/lol/summoner/v4/summoners/by-account/{encryptedAccountId}",
        "method": "GET",
        "param": [
          "{encryptedAccountId}"
        ]
      }
    },
    "match": {
      "host": "REGIONAL",
      "byPuuid": {
        "path": "/lol/match/v5/matches/by-puuid/{puuid}/ids",
        "method": "GET",
        "param": [
          "{puuid}"
        ],
        "qs": [
          "startTime",
          "endTime",
          "queue",
          "type",
          "start",
          "count"
        ]
      },
      "matches": {
        "path": "/lol/match/v5/matches/{matchId}",
        "method": "GET",
        "param": [
          "{matchId}"
        ]
      },
      "timeline": {
        "path": "/lol/match/v5/matches/{matchId}/timeline",
        "method": "GET",
        "param": [
          "{matchId}"
        ]
      }
    },
    "championMastery": {
      "host": "PLATFORM",
      "bySummoner": {
        "path": "/lol/champion-mastery/v4/champion-masteries/by-summoner/{encryptedSummonerId}",
